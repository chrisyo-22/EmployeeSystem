spring.application.name=EmployeeManagementSystem

spring.datasource.url=jdbc:mysql://localhost:3306/employeesystem
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
spring.datasource.username=root
spring.datasource.password=chris

#配置SQL日志打印在控制台
mybatis.configuration.log-impl=org.apache.ibatis.logging.stdout.StdOutImpl
#开启驼峰命名规则映射 例子：abc_xyz --> abcXyz
mybatis.configuration.map-underscore-to-camel-case=true
#Open debug for Transactional logging
logging.level.org.springframework.jdbc.support.JdbcTransactionManager=debug

#Set maximum file file size
spring.servlet.multipart.max-file-size=10MB
spring.servlet.multipart.max-request-size=10MB

aws.region=us-east-1
aws.s3.bucket=amz-chris-yo-22-wode-mingzi-hello-world